{
  "name": "create-mfe",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Fredrik LÃ¶wenhamn",
  "bin": "index.js",
  "files": [
    "index.js",
    "dist",
    "templates"
  ],
  "scripts": {
    "start": "rm -rf mfe-project && tsx src/index.ts",
    "build": "pnpm templates && unbuild",
    "templates": "rm -rf templates && rsync -r --exclude dist --exclude node_modules ../../templates . && node ./versions.js",
    "typecheck": "tsc --noEmit",
    "npm-publish": "pnpm build && npm publish",
    "test": "vitest",
    "puppeteer": "node node_modules/puppeteer/install.mjs",
    "test:prepare": "node test/setup.js",
    "test:run": "pnpm test:prepare && pnpm test run && pnpm test:clean",
    "test:clean": "rm -rf mfe-test-* && rm -rf templates"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.3",
    "@types/prompts": "^2.4.6",
    "@types/cross-spawn": "^6.0.3",
    "cross-spawn": "^7.0.3",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "tsx": "^3.13.0",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6",
    "vite": "^4.5.0",
    "puppeteer": "^21.3.8"
  }
}
